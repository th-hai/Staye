{
  "kind": "collectionType",
  "collectionName": "rooms",
  "info": {
    "name": "Rooms",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "address": {
      "type": "string"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "services": {
      "collection": "services"
    },
    "price": {
      "type": "biginteger"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "checked",
        "available"
      ]
    },
    "location": {
      "model": "locations"
    },
    "owner": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "rooms"
    }
  }
}
